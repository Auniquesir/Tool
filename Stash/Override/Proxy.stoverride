name: "Proxy"
desc: "Proxy分流规则"

rule-providers:
  Proxy:
    behavior: classical
    url: "https://raw.githubusercontent.com/Auniquesir/Tool/X/Stash/Rules/Proxy/Proxy.yaml"
    interval: 86400

  Proxy_Domain:
    behavior: domain
    url: "https://raw.githubusercontent.com/Auniquesir/Tool/X/Stash/Rules/Proxy/Proxy_Domain.yaml"
    interval: 8640

rules:
  - RULE-SET,Proxy,Proxy
  - RULE-SET,Proxy_Domain,Proxy
