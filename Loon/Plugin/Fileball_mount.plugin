#!name = Fileball网盘挂载
#!desc = 支持挂载阿里云盘、夸克网盘和PikPak网盘，点击下方署名查看使用方法。
#!icon = https://gitlab.com/lodepuly/iconlibrary/-/raw/main/App_icon/120px/Fileball.png

[General]
force-http-engine-hosts = aliyun.example.com:0, quark.example.com:0, pikpak.example.com:0

[Script]
http-request ^http:\/\/(aliyun|quark|pikpak)\.example\.com script-path = https://raw.githubusercontent.com/Auniquesir/Tool/X/Loon/Script/Fileball/Fileball_mount.js, requires-body = true, tag = 三合一网盘挂载
